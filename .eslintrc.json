{
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["github"],
  "extends": ["plugin:github/recommended"],
  "rules": {
    "camelcase": ["error", {"allow": ["per_page", "record_delimiter"]}],
    "no-console": "off",
    "i18n-text/no-en": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off"
  }
}
